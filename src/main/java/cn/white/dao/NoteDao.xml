<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.white.dao.NoteDao">
    <insert id="saveNote" parameterType="cn.white.entity.Note">
      INSERT INTO cn_note(cn_note_id,cn_user_id,cn_notebook_id,cn_note_title,cn_note_body,cn_note_createtime)
      VALUES (#{cn_note_id},#{cn_user_id},#{cn_notebook_id},#{cn_note_title},#{cn_note_body},#{cn_note_createtime})
    </insert>

</mapper>

